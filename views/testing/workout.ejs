<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Workout Plan</title>
    <link rel="stylesheet" href="/styles.css">
</head>
<body>
    <h1>Generated Workout Plan</h1>
    <ul>
        <% workoutPlan.forEach((exercise) => { %>
            <li><%= exercise.name %> - <%= exercise.description %> - <%= exercise.muscleGroup %></li>
        <% }) %>
    </ul>
</body>
</html>
